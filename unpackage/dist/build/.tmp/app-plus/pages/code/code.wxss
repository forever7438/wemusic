.code_success {
  padding: 64rpx 30rpx 40rpx 30rpx;
  background-color: #333;
  height: 100vh;
}
.code_success .code_content {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  width: 100%;
  background-color: #fff;
  border-radius: 12rpx;
}
.code_success .code_content text:nth-of-type(1) {
  margin: 60rpx auto 14rpx;
  font-size: 48rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #333333;
}
.code_success .code_content text:nth-of-type(2) {
  margin-bottom: 80rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #909090;
}
.code_success .code_content .qrcode {
  width: 348rpx;
  height: 348rpx;
  margin-bottom: 164rpx;
}

