<view class="lesson_teacher" bind:__l="__l"><text class="teacher_class">授课教师</text><swiper class="swiper" indicator-dots="{{indicatorDots}}" display-multiple-items="2" next-margin="90px" autoplay="{{autoplay}}" interval="{{interval}}"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="teacher-item"><navigator style="height:120rpx;" hover-class="none" url="{{'/pages/teacherDetail/teacherDetail?teacherId='+item.$orig.id}}"><image src="{{item.$orig.photo||image}}"></image></navigator><text class="teacher_name">{{item.$orig.name}}</text><startclass star="{{item.m0}}" starColor="#666666"></startclass></view></swiper-item></block></swiper></view>